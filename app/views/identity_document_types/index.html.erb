<p id="notice"><%= notice %></p>

<h1>Identity Document Types</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identity_document_types.each do |identity_document_type| %>
      <tr>
        <td><%= identity_document_type.uid %></td>
        <td><%= identity_document_type.name %></td>
        <td><%= identity_document_type.description %></td>
        <td><%= identity_document_type.status %></td>
        <td><%= identity_document_type.account_id %></td>
        <td><%= link_to 'Show', identity_document_type %></td>
        <td><%= link_to 'Edit', edit_identity_document_type_path(identity_document_type) %></td>
        <td><%= link_to 'Destroy', identity_document_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identity Document Type', new_identity_document_type_path %>
